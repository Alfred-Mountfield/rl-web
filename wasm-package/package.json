{
  "name": "wasm-package",
  "version": "0.0.0-local",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/umd/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/umd/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "node": "./dist/node/index.cjs",
      "import": "./dist/es/index.js",
      "default": "./dist/cjs/index.cjs"
    },
    "./slim": {
      "types": "./dist/cjs-slim/index_slim.d.ts",
      "node": "./dist/node/index.cjs",
      "import": "./dist/es-slim/index_slim.js",
      "default": "./dist/cjs-slim/index_slim.cjs"
    },
    "./index_bg.wasm": "./dist/index_bg.wasm",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && npm run build:wasm && npm run build:bundle",
    "build:wasm": "(cd ../wasm-crate && wasm-pack build -t web --out-dir ../main/wasm)",
    "build:bundle": "rollup -c --bundleConfigAsCjs",
    "compressed-size": "npm run build && find dist -iname '*.js' -exec npx terser@latest --compress --mangle --output {} -- {} \\;",
    "optimize": "wasm-opt -Os src/wasm/index_bg.wasm -o tmp.wasm && mv tmp.wasm src/wasm/index_bg.wasm",
    "test": "tsc --outDir out",
    "prepublishOnly": "npm test && npm run optimize && npm run build:bundle"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.1",
    "@rollup/plugin-wasm": "^6.0.0",
    "@types/node": "^18.8.4",
    "rollup": "^3.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  }
}
